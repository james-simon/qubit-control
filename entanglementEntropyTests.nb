Cell["entropy from ZX interaction with noncommuting term", "Section",
 CellChangeTimes->{{3.7754431696992764`*^9, 3.775443200486563*^9}}],

Cell[CellGroupData[{

Cell[BoxData[{
 RowBox[{
  RowBox[{"H", " ", "=", " ", 
   RowBox[{
    RowBox[{"TProduct", "[", 
     RowBox[{"\[Sigma]Z", ",", " ", "\[Sigma]Z"}], "]"}], " ", "+", " ", 
    RowBox[{".5", "*", 
     RowBox[{"TProduct", "[", 
      RowBox[{"\[Sigma]X", ",", " ", "\[Sigma]I"}], "]"}]}]}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"U", "[", "t_", "]"}], " ", ":=", " ", 
  RowBox[{"MatrixExp", "[", 
   RowBox[{
    RowBox[{"-", "\[ImaginaryI]"}], "*", "t", "*", "H"}], 
   "]"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Tmax", " ", "=", " ", "3"}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"ts", " ", "=", " ", 
   RowBox[{"Table", "[", 
    RowBox[{"t", ",", " ", 
     RowBox[{"{", 
      RowBox[{"t", ",", " ", "0", ",", " ", "Tmax", ",", " ", 
       RowBox[{"Tmax", "/", "100"}]}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"entropies", " ", "=", " ", 
    RowBox[{"Table", "[", 
     RowBox[{
      RowBox[{"operatorEntropy", "[", 
       RowBox[{"U", "[", "t", "]"}], "]"}], ",", " ", 
      RowBox[{"{", 
       RowBox[{"t", ",", " ", "ts"}], "}"}]}], "]"}]}], ";"}], 
  "\[IndentingNewLine]"}], "\[IndentingNewLine]", 
 RowBox[{"ListPlot", "[", 
  RowBox[{
   RowBox[{"Transpose", "[", 
    RowBox[{"{", 
     RowBox[{"ts", ",", " ", "entropies"}], "}"}], "]"}], ",", " ", 
   RowBox[{"PlotLabel", "\[Rule]", "\"\<small noncommuting term\>\""}], ",", 
   " ", 
   RowBox[{"AxesLabel", "\[Rule]", 
    RowBox[{"{", 
     RowBox[{"\"\<t\>\"", ",", " ", "\"\<H(U(t))\>\""}], "}"}]}]}], 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{"Print", "[", 
  RowBox[{"\"\<max entropy: \>\"", " ", "<>", " ", 
   RowBox[{"ToString", "[", 
    RowBox[{"Max", "[", "entropies", "]"}], "]"}]}], "]"}]}], "Input",
